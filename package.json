{
  "name": "dansdata-api",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "prisma generate",
    "dev": "dotenv -e .env.development.local nodemon -- --watch 'src/**/*' --ignore '**/*.test.ts' --ignore '**/*.test.tsx' --exec \"ts-node\" src/__build__/api_docs.ts & next dev",
    "build": "ts-node src/__build__/api_docs.ts && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write . && prisma format ./prisma",
    "test:unit": "jest --group=unit",
    "test:integration": "jest --group=integration",
    "test:all": "jest",
    "dev:db:generate": "prisma generate",
    "dev:db:push": "dotenv -e .env.development.local prisma migrate deploy && dotenv -e .env.development.local prisma db push",
    "dev:db:studio": "dotenv -e .env.development.local prisma studio",
    "dev:vercel:pull": "vercel env pull .env.development.local"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^5.2.0",
    "@prisma/client": "^5.0.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "20.3.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@vercel/analytics": "^1.0.1",
    "@vercel/postgres": "^0.4.0",
    "daisyui": "^3.1.6",
    "eslint-config-next": "13.4.7",
    "jest-runner-groups": "^2.2.0",
    "next": "13.4.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.32.1",
    "swagger-ui-react": "^5.1.0",
    "wretch": "^2.5.2",
    "zod": "^3.21.4",
    "zod-validation-error": "^1.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@ts-safeql/eslint-plugin": "^1.1.4",
    "@types/swagger-ui-react": "^4.18.0",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "autoprefixer": "^10.4.14",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-mock-extended": "^3.0.4",
    "libpg-query": "^15.0.2",
    "lint-staged": "^13.2.2",
    "node-mocks-http": "^1.12.2",
    "nodemon": "^2.0.22",
    "path-equal": "^1.2.5",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-sh": "^0.12.8",
    "prisma": "^5.0.0",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "vercel": "^30.2.3"
  }
}
