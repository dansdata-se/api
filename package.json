{
  "name": "dansdata-api",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "prisma generate",
    "dev": "ts-node src/__build__/load_envvars.ts nodemon -- --watch 'src/**/*' --ignore '**/*.test.ts' --ignore '**/*.test.tsx' --exec \"ts-node\" src/__build__/api_docs.ts & next dev",
    "build": "ts-node src/__build__/api_docs.ts && next build",
    "start": "next start",
    "lint": "tsc && next lint",
    "format": "prettier --write . && prisma format ./prisma",
    "test:unit": "jest --group=unit",
    "test:integration": "jest --group=integration",
    "test:all": "jest",
    "dev:db:generate": "prisma generate",
    "dev:db:push": "ts-node src/__build__/load_envvars.ts prisma migrate deploy && ts-node src/__build__/load_envvars.ts prisma db push",
    "dev:db:migrate": "ts-node src/__build__/load_envvars.ts prisma migrate",
    "dev:db:studio": "ts-node src/__build__/load_envvars.ts prisma studio"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^6.3.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^5.5.2",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "20.8.9",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@vercel/analytics": "^1.1.1",
    "@vercel/postgres": "^0.5.0",
    "@vercel/speed-insights": "^1.0.1",
    "daisyui": "^3.9.4",
    "dayjs": "^1.11.10",
    "envsafe": "^2.0.3",
    "eslint-config-next": "14.0.0",
    "geolib": "^3.3.4",
    "jest-runner-groups": "^2.2.0",
    "next": "14.0.0",
    "pino": "^8.16.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.32.6",
    "swagger-ui-react": "^5.10.3",
    "wretch": "^2.7.0",
    "zod": "^3.22.4",
    "zod-validation-error": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@faker-js/faker": "^8.2.0",
    "@testcontainers/postgresql": "^10.2.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/async-lock": "^1.4.2",
    "@types/cross-spawn": "^6.0.4",
    "@types/jest": "^29.5.6",
    "@types/swagger-ui-react": "^4.18.2",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "async-lock": "^1.4.0",
    "autoprefixer": "^10.4.16",
    "cross-spawn": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-mock-extended": "^3.0.5",
    "lint-staged": "^15.0.2",
    "minimist": "^1.2.8",
    "node-mocks-http": "^1.13.0",
    "nodemon": "^3.0.1",
    "path-equal": "^1.2.5",
    "pino-pretty": "^10.2.3",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-sh": "^0.13.1",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "prisma": "^5.5.2",
    "tailwindcss": "^3.3.5",
    "testcontainers": "^10.2.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "vercel": "^32.5.0"
  }
}
